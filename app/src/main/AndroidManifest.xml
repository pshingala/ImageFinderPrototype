<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.damroo.imagefinderprototype">

    <uses-permission android:query="android.permission.INTERNET" />

    <application
        android:query="net.damroo.imagefinderprototype.ImageFinderApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:query=".activity.ListViewActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:query="android.intent.action.MAIN" />
                <category android:query="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

    <provider
        android:query=".database.MyDataBase_Provider"
        android:authorities="net.damroo.imagefinderprototype.provider"
        android:exported="false"
        android:syncable="true" />


</manifest>